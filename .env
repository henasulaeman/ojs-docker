# Set your journal Tag:
COMPOSE_PROJECT_NAME=journal
PROJECT_DOMAIN=journal.localhost
SERVERNAME=$PROJECT_DOMAIN

OJS_IMAGE=ojs
OJS_VERSION=3_3_0-14

# IMPORTANT: Set a non used port.
HTTP_PORT=8081
HTTPS_PORT=8481

# Remember this to fill your installation form in a clean installatio
# MYSQL_VERSION=10.3
MYSQL_ROOT_PASSWORD=change_your_pwd

# OJS variables
OJS_CLI_INSTALL=1
OJS_DB_HOST=db
OJS_DB_DRIVER=mysqli
OJS_DB_USER=ojs 
OJS_DB_PASSWORD=change_your_pwd
OJS_DB_NAME=ojs 
